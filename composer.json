{
    "version": "0.0.1",
    "name": "mhbarry/laravelresourcefy",
    "description": "Build resources easly",
    "license": "proprietary",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/mhbarry/laravelresourcefy",
    "keywords": ["Laravel", "LaravelResourcefy"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Mhbarry\\LaravelResourcefy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhbarry\\LaravelResourcefy\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mhbarry\\LaravelResourcefy\\LaravelResourcefyServiceProvider"
            ],
            "aliases": {
                "LaravelResourcefy": "Mhbarry\\LaravelResourcefy\\Facades\\LaravelResourcefy"
            }
        }
    }
}
